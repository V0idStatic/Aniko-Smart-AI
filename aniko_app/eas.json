{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
         "EXPO_PUBLIC_OPENROUTER_API_KEY": "@EXPO_PUBLIC_OPENROUTER_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
       "env": {
        "EXPO_PUBLIC_OPENROUTER_API_KEY": "@EXPO_PUBLIC_OPENROUTER_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
       "env": {
        "EXPO_PUBLIC_OPENROUTER_API_KEY": "@EXPO_PUBLIC_OPENROUTER_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
